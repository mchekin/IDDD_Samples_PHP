{
  "name": "mchekin/implementing-ddd",
  "description": "Sample project implementing DDD",
  "type": "project",
  "require-dev": {
    "phpunit/phpunit": "^8.3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Chekin",
      "email": "mchekin@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "nesbot/carbon": "^2.24",
    "ramsey/uuid": "^3.8",
    "webmozart/assert": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.2.11"
    },
    "sort-packages": true
  }
}
